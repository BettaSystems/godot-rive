{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/godot-cpp/include",
                "${workspaceFolder}/godot-cpp/gen/include",
                "${workspaceFolder}/thirdparty/rive-cpp",
                "${workspaceFolder}/thirdparty/rive-cpp/include",
                "${workspaceFolder}/godot-cpp/gdextension",
                "${workspaceFolder}/thirdparty/rive-cpp/skia/dependencies/skia"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/src",
                    "${workspaceFolder}/godot-cpp/include",
                    "${workspaceFolder}/godot-cpp/gen/include",
                    "${workspaceFolder}/thirdparty/rive-cpp/include",
                    "${workspaceFolder}/thirdparty/rive-cpp/skia/dependencies/skia/include",
                    "${workspaceFolder}/thirdparty/rive-cpp/skia/renderer/include"
                ]
            }
        }
    ],
    "version": 4
}